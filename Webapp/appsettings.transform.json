{
    "MongoConnection": {
        "ConnectionString": "mongodb://[username]:[password]@[host]:[port]/[db_name]",
        "Database": "[db_name]"
    },

    "Logging": {
        "IncludeScopes": false,
        "Debug": {
            "LogLevel": {
                "Default": "Warning"
            }
        },
        "Console": {
            "LogLevel": {
                "Default": "Warning"
            }
        }
    }
}